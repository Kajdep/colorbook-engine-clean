# ColorBook Engine Development Environment
# Copy this file to .env for local development

# Frontend Configuration
VITE_APP_NAME=ColorBook Engine
VITE_API_URL=http://localhost:3001/api
VITE_FRONTEND_URL=http://localhost:3000

# Google AI Configuration (Get from Google Cloud Console)
VITE_GOOGLE_CLOUD_PROJECT_ID=your-project-id
VITE_GOOGLE_SERVICE_ACCOUNT_KEY=your-service-account-json-key

# Stripe Configuration (Optional for testing payments)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
VITE_STRIPE_PRO_PRICE_ID=price_your-pro-price-id
VITE_STRIPE_ENTERPRISE_PRICE_ID=price_your-enterprise-price-id

# Feature Flags
VITE_ENABLE_PAYMENTS=false
VITE_ENABLE_GOOGLE_AI=true
VITE_ENABLE_OPENAI=true
VITE_ENABLE_STABILITY_AI=true

# Local encryption secret for storing API keys securely
VITE_ENCRYPTION_SECRET=replace-with-32-character-secret

# Development Settings
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

